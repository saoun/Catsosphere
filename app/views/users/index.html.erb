<%= render "posts/navbar" %>

<div class="profile">


<h1> View Users </h1>
<div class="dividerComment"></div>

<% @users.each do |user| %>
    <div class="optionsMiddle">
    <a href="users/<%=user['id']%>"><%= user['fullname']%></a><br>
    <strong>Contact: </strong><p> <%= user['contact'] %></p></br>
    <strong>Bio: </strong><p> <%= user['bio'] %></p></br>
    <div class="dividerComment"></div>
    </div>
  <% end %>

</div>
