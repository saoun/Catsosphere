<%= render "posts/navbar" %>

<div class="container">


<h1> View Users </h1>
<div class="dividerComment"></div>

<% @users.each do |user| %>
    <div class="optionsMiddle">
    <a href="users/<%=user['id']%>"><%= user['fullname']%></a>
    <strong>Contact: </strong><p> <%= user['contact'] %></p>
    <strong>Bio: </strong><p> <%= user['bio'] %></p>
    <div class="dividerComment"></div>
    </div>
  <% end %>

</div>
